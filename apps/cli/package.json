{
	"name": "@go-kickstart/cli",
	"private": true,
	"scripts": {
		"install": "go mod download",
		"dev": "go run ./cmd/gokickstart",
		"build": "go build -o ../../dist/gokickstart ./cmd/gokickstart",
		"test": "go test ./...",
		"lint": "go vet ./...",
		"format:check": "test -z \"$(gofmt -l .)\"",
		"format:fix": "gofmt -w .",
		"clean": "rm -rf ../../dist"
	}
}
